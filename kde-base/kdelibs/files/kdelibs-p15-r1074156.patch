+    color: #ffffff;
+    background: url(block_title_bottom.png) no-repeat bottom right;
+}
+
+.clearer {
+    clear: both;
+    height: 1px;
+}
+
+
+/*
+** Footer
+*/
+#footer {
+    display: none;
+        background: #eeeeee;
+}
+
+#footer_text {
+    margin: 0 auto;
+    padding: 1em 0 1em 3.5em;
+        text-align: left;
+    color: #000000;
